<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.example.apitest.Mapper.board.BoardMapper">
    <select id="getBoardList"
            resultType="org.example.apitest.model.board.Board">
        SELECT ID, TITLE, CRE_ID, CRE_DTM
        FROM TB_BOARD
    </select>

    <insert id="insertBoard"
            parameterType="String">
        INSERT INTO TB_BOARD (TITLE, CRE_ID, CRE_DTM)
        VALUES (#{title}, '테스트계정임', NOW())
    </insert>
</mapper>